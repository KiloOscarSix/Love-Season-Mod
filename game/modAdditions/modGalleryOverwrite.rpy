screen replay():
    tag menu
    python:
        galleryList = [
        [persistent.extra_heather, "images/gallery/extra1_heather.png", "images/gallery/extra1_heather_hover.png", "HeatherExtra", "Extra 1 - Heather"],
        [persistent.laura_first, "images/gallery/laura_first.png", "images/gallery/laura_first_hover.png", "AfterParty", "Laura - First Time"],
        [persistent.nina_first, "images/gallery/nina_first.png", "images/gallery/nina_first_hover.png", "Disinfect", "Nina - First Time"],
        [persistent.selena_first, "images/gallery/ch3_28.png", "images/gallery/ch3_28_hover.png", "MeetingSelena", "Selena - Meeting"],
        [persistent.nova_sleep, "images/gallery/ch3_55.png", "images/gallery/ch3_55_hover.png", "Day5Sleep", "Nova - Teasing"],
        [persistent.elise_facial, "images/gallery/ch3_68.png", "images/gallery/ch3_68_hover.png", "Day6EliseFacial2", "Elise - Facial 1"],
