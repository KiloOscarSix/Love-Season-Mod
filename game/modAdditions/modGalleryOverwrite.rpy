    add "gallery/scenes_bg.jpg"

    vpgrid:
        ypos 0.14
        ysize 0.75
        cols 4
        spacing 10
        draggable True
        mousewheel True

        scrollbars "vertical"
        side_xalign 0.5

        for i in galleryList:
            vbox xalign 0.5 yalign 0.5:
                imagebutton:
                    focus_mask True
                    idle i[1]
                    hover i[2]
                    insensitive "gallery/locked.png"
                    action Replay(i[3], locked=False)
                if i[0] or persistent.galleryUnlocked:
                    text "{size=-4}[i[4]]{/size}" xalign 0.5 yalign 0.5 ypos 0.7
                else:
                    text "Locked" xalign 0.5 yalign 0.5 ypos 0.7

        for i in range(12-len(galleryList)):
            vbox xalign 0.5 yalign 0.5:
                add "gallery/disabled_btn.png"
                text ""

    vbox xalign 0.45 yalign 1.03:
        vbox xalign 0.5 yalign 1.0:
            textbutton _("{size=+25}Return{/size}"):
                style "return_button"
                action ShowMenu("gallery")
